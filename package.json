{
  "name": "PixivDeck",
  "productName": "PixivDeck",
  "version": "0.12.1",
  "description": "pixiv client for desktop",
  "keywords": [
    "electron",
    "pixiv"
  ],
  "homepage": "https://github.com/akameco/PixivDeck",
  "repository": "github:akameco/PixivDeck",
  "author": "akameco <akameco.t@gmail.com>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "add": "all-contributors add",
    "generate": "all-contributors generate",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "dev": "cross-env START_HOT=1 npm run hot-updates-server",
    "test": "npm run jest",
    "test:ci": "cross-env NODE_ENV=test npm run flow && npm run jest --ci",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "jest": "cross-env NODE_ENV=test jest",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "build": "npm run build:main && npm run build:renderer",
    "s2s": "babel-node ./config/scripts/s2s.js",
    "flow": "flow",
    "i18n": "babel-node ./config/scripts/extract-intl.js",
    "build:dll": "node config/scripts/webpack build-dll",
    "build:main": "node config/scripts/webpack build-main",
    "build:renderer": "node config/scripts/webpack build-renderer",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint \"app/**/*.js\"",
    "lint:js": "eslint .",
    "hot-updates-server": "node config/scripts/webpack hot-server",
    "start-hot-renderer": "node config/scripts/hot-renderer",
    "postinstall": "npm run build:dll && electron-builder install-app-deps",
    "package": "npm run build && build --publish never",
    "release": "npm run build && build"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": "stylelint-config-standard",
    "syntax": "scss",
    "rules": {
      "rule-empty-line-before": null,
      "at-rule-empty-line-before": null
    }
  },
  "dependencies": {
    "autolinker": "^1.6.2",
    "axios": "^0.17.1",
    "camelcase-keys": "^4.2.0",
    "decamelize-keys": "^1.1.0",
    "electron-debug": "^2.1.0",
    "keycode": "^2.2.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "material-ui": "^0.20.0",
    "ms": "^2.1.1",
    "normalizr": "^3.2.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-event-listener": "^0.5.3",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-sortable-pane": "^0.6.8",
    "redux": "^3.7.1",
    "redux-persist": "^4.10.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.4.0",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-create-redux-action-func": "^0.1.4",
    "babel-plugin-create-redux-action-type": "^0.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-react-intl-auto": "^1.1.0",
    "babel-plugin-redux-action-compose": "^0.3.0",
    "babel-plugin-redux-state-compose": "^0.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "boiler-room-custodian": "^0.6.2",
    "chalk": "^2.4.2",
    "concurrently": "^3.6.0",
    "cp-file": "^5.0.0",
    "cross-env": "^5.1.6",
    "cross-spawn": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.6",
    "electron-builder": "^20.26.1",
    "electron-load-devtool": "^1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.8.0",
    "eslint-config-precure": "^3.3.0",
    "execa": "^0.8.0",
    "extract-react-intl": "^0.6.0",
    "extract-react-intl-messages": "^0.10.0",
    "extract-zip": "^1.6.7",
    "fbjs-scripts": "^0.8.3",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.63.1",
    "flow-runtime": "^0.16.0",
    "husky": "^1.3.1",
    "jest": "^23.4.2",
    "jest-styled-components": "^5.0.1",
    "jest-yaml-flat-transfrom": "^0.1.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "raf": "^3.4.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.8.3",
    "read-pkg-up": "^4.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-styled-components": "^1.3.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "yaml-flat-loader": "^0.1.0"
  },
  "devEngines": {
    "node": ">=7",
    "npm": ">=3.x",
    "yarn": ">=0.22.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run flow && lint-staged"
    }
  }
}
